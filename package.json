{
  "name": "vue-accordion-transpiled",
  "version": "1.0.0",
  "description": "",
  "main": "dist/vue-accordion-transpiled.ssr.js",
  "module": "dist/vue-accordion-transpiled.esm.js",
  "unpkg": "dist/vue-accordion-transpiled.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue",
    "!src/serve-dev.*"
  ],
  "scripts": {
    "serve": "vue-cli-service serve src/serve-dev.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {
    "badger-accordion": "^1.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/plugin-transform-arrow-functions": "^7.7.4",
    "@babel/plugin-transform-parameters": "^7.7.7",
    "@babel/plugin-transform-shorthand-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-replace": "^2.2.1",
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "babel-plugin-transform-es2015-shorthand-properties": "^6.24.1",
    "cross-env": "^6.0.3",
    "minimist": "^1.2.6",
    "rollup": "^1.27.13",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-vue": "5.1.1",
    "sass-loader": "^8.0.0",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "peerDependencies": {
    "vue": "^2.6.10"
  },
  "engines": {
    "node": ">=8"
  }
}
